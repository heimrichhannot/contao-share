{
  "name": "heimrichhannot/contao-share-bundle",
  "type": "contao-bundle",
  "description": "A bundle to provide extended pdf, print and social share functionality for contao.",
  "keywords": [
    "contao",
    "symfony",
    "share",
    "facebook",
    "googleplus",
    "twitter",
    "pdf"
  ],
  "homepage": "https://github.com/heimrichhannot/contao-share",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Heimrich & Hannot",
      "email": "digitales@heimrich-hannot.de",
      "homepage": "http://www.heimrich-hannot.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/heimrichhannot/contao-share/issues"
  },
  "require": {
    "php": "~7.1",
    "contao/core-bundle": "~4.4",
    "heimrichhannot/contao-request": "^1.1"
  },
  "require-dev": {
    "contao/test-case": "^1.1",
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "phpunit/phpunit": "^6.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "suggest": {
    "mpdf/mpdf": "~6.1"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\ShareBundle\\": "src/"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "contao-manager-plugin": "HeimrichHannot\\SpeedBundle\\ContaoManager\\Plugin"
}
